{
  "name": "GitDrive",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ysabri/GitDrive"
  },
  "description": "A typescript Git cloud drive app",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json build/**/* -p tsconfig.json",
    "start": "yarn clean && yarn build && yarn lint && node .electron-vue/dev-runner.js",
    "test": "mocha -t 10000 -r ts-node/register --full-trace --check-leaks ./src/tests/**/*.ts ./src/tests/*.ts",
    "clean": "rimraf build/*",
    "dist": "yarn dist:clean && yarn build && node .electron-vue/build.js && electron-builder",
    "dist:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dist:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "pack": "yarn pack:main && yarn pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js"
  },
  "main": "build/main/main.js",
  "keywords": [
    "Git",
    "drive",
    "cloud",
    "collaboration"
  ],
  "author": {
    "name": "Yazeed Sabri",
    "email": "ysabri@wisc.edu",
    "url": "https://github.com/ysabri"
  },
  "build": {
    "productName": "git-drive",
    "appId": "ysabri.git-drive",
    "directories": {
      "output": "packaged"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "static/icons/icon.icns"
    },
    "win": {
      "icon": "static/icons/icon.ico"
    },
    "linux": {
      "icon": "static/icons"
    }
  },
  "baseUrl": "./src",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/fs-extra": "^5.0.1",
    "@types/google-protobuf": "^3.2.7",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.1",
    "@types/source-map-support": "^0.4.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "dugite": "^1.57.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.5.1",
    "electron-debug": "^2.0.0",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mocha": "^5.0.4",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "performance-now": "^2.1.0",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "spectron": "^3.7.1",
    "style-loader": "^0.21.0",
    "ts-loader": "3.5.0",
    "ts-node": "^6.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "url-loader": "^1.0.1",
    "vue-class-component": "^6.2.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.4.2",
    "vuex-typescript": "^3.0.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.7.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "fs-extra": "^6.0.1",
    "google-protobuf": "^3.5.0",
    "source-map-support": "^0.5.4",
    "vue": "^2.5.16",
    "vuex": "^3.0.1"
  }
}
