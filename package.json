{
  "name": "GitDrive",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ysabri/GitDrive"
  },
  "description": "A typescript Git cloud drive app",
  "scripts": {
    "compile": "electron-webpack",
    "build": "tsc",
    "lint": "tslint -c tslint.json ts-src/**/* -p tsconfig.json",
    "start": "yarn clean && yarn build && electron-webpack dev",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register --full-trace --check-leaks ./src/tests/**/*.ts ./src/tests/*.ts",
    "clean": "rimraf build/*",
    "dist": "yarn compile && electron-builder"
  },
  "keywords": [
    "Git",
    "drive",
    "cloud",
    "collaboration"
  ],
  "author": {
    "name": "Yazeed Sabri",
    "email": "ysabri@wisc.edu",
    "url": "https://github.com/ysabri"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "chai": "^4.1.2",
    "dugite": "^1.57.0",
    "electron": "1.8.4",
    "mocha": "^5.0.4",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "electron-builder": "^20.5.1",
    "electron-webpack": "^1.13.0",
    "webpack": "^3.11.0",
    "performance-now": "^2.1.0"
  },
  "dependencies": {
    "protobufjs": "^6.8.6",
    "source-map-support": "^0.5.4"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "electronWebpack": {
    "commonSourceDirectory": "build/common",
    "main": {
      "sourceDirectory": "build/main"
    },
    "renderer": {
      "sourceDirectory": "build/renderer"
    }
  }
}
