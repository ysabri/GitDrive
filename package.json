{
  "name": "GitDrive",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/ysabri/GitDrive"
  },
  "description": "A typescript Git cloud drive app",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json src/**/* -p tsconfig.json",
    "start": "npm run clean && npm run build && electron ./build/main.js",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register --full-trace --check-leaks ./src/tests/**/*.ts ./src/tests/*.ts",
    "clean": "rimraf build/*"
  },
  "keywords": [
    "Git",
    "drive",
    "cloud",
    "collaboration"
  ],
  "author": {
    "name": "Yazeed Sabri",
    "email": "ysabri@wisc.edu",
    "url": "https://github.com/ysabri"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.53",
    "chai": "^4.1.2",
    "dugite": "^1.57.0",
    "electron": "^1.7.11",
    "mocha": "^5.0.4",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "dependencies": {}
}
